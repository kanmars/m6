<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.kanmars.dao">
	<class
		name="Project"
		table="project"
	>
		<id
			name="Id"
			type="string"
			column="ID"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="Userid"
			column="USERID"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Name"
			column="NAME"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Aspect"
			column="ASPECT"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Updopr"
			column="UPDOPR"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Updtime"
			column="UPDTIME"
			type="timestamp"
			not-null="false"
			length="19"
		/>


	</class>	
</hibernate-mapping>